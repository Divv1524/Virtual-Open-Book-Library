package com.virtual.openbook.library.books.dto;

import lombok.Data;
@Data
public class UserDTO {

    private Long id;
    private String name;
    private int downloadCount;

public UserDTO() {}

    public UserDTO(Long id, String name, int downloadCount) {
        this.id = id;
        this.name = name;
        this.downloadCount = downloadCount;
    }
public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }
public void setName(String name) {
        this.name = name;
    }

    public int getDownloadCount() {
        return downloadCount;
    }

    public void setDownloadCount(int downloadCount) {
        this.downloadCount = downloadCount;
    }
}
